@import "../tpls/base.wxss";
@import "../tpls/products/ThemeProducts/ThemeProducts-tpl.wxss";
.services_container{
  width:750rpx;
  display: flex;
  flex-direction:column;
}

.services_banner{
  width:100%;
  height:320rpx;
}

.menus{
  display: flex;
  flex-direction:row;
  width:100%;
  height: 80rpx;
  background-color: #f0f0f0;
}

.menus_titile{
  width:30%;
  height: 80rpx;
  margin-left: 80rpx;
}

.menus_titile text{
  font-size: 14px;
  color: #333333;
  line-height: 80rpx;
}

.menus_titile image{
  width:28rpx;
  height:28rpx;
}
